    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/websockets": "^10.3.10",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.15.1",
    "cache-manager": "^5.7.3",
    "cache-manager-redis-yet": "^5.1.3",
    "crawlee": "^3.11.3",
    "ghost-cursor-playwright": "^2.1.0",
    "mongoose": "^8.5.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "playwright": "*",
