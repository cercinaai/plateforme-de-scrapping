  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:crawler:bienici": "npx ts-node src/crawler/bienici/bienici.crawler.test.ts",
    "test:crawler:logicimmo": "npx ts-node src/crawler/logic-immo/logicimmo.crawler.test.ts",
    "test:crawler:boncoin": "npx ts-node src/crawler/boncoin/boncoin.crawler.test.ts",
    "test:crawler:seloger": "npx ts-node src/crawler/seloger/seloger.crawler.test.ts"
  },
  "dependencies": {
    "@bull-board/api": "^5.21.1",
